package ch4;
import java.util.Random;
import java.util.random.*;
public class TossCoin {
	public static void main(String[] args) {
		
		
		
		Random rand = new Random();
		
		for(int i =0; i<=10; i++) {
			if (rand.nextInt(2) == 0) {
				System.out.println("Tails");
			}
			else {
				System.out.println("Heads");
			}
			
			
			
			
		}
	}

}
